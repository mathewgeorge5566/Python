    "print(\"Enter the value for n\")\n",
    "n=int(input())\n",
    "r=0\n",
    "s=0\n",
    "i=0\n",
    "while(r is not n):\n",
    "    i=i+1\n",
    "    c=0\n",
    "    for k in range(1,i+1):\n",
    "        if(i%k==0):\n",
    "            c=c+1\n",
    "    if(c==2):\n",
    "        s=i\n",
    "        r=r+1\n",
    "if(r==n):\n",
    "     print(\"Output:\",s)"
