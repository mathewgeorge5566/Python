    "import sympy\n",
    "n=int(input())\n",
    "sum_fact=0\n",
    "a=[]\n",
    "a=sympy.primepi(n)\n",
    "while(a>0):\n",
    "    r=a%10\n",
    "    sum_fact=sum_fact+r\n",
    "    a=a//10\n",
    "print( sum_fact)"
